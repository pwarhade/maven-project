pipeline
{

agent any
    

    stages{
            stage('Build'){
                            steps{
                                    git 'https://github.com/pwarhade/maven-project.git'
                                 }
                            }


            stage ("code test") {
                                steps {
                                    withMaven(maven: 'mymaven') 
                                       {
                                             sh 'mvn test'
                                        }
                                      }

                                }

                    }
    }
